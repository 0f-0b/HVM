@main = d
  & @sum ~ (27 (c d))
  & @gen ~ (27 (0 c))

@gen = (?(((a a) @gen$C0) b) b)

@gen$C0 = ({a d} ({$([*2] $([+1] b)) $([*2] e)} (c f)))
  & @gen ~ (a (b c))
  &!@gen ~ (d (e f))

@sum = (?(((a a) @sum$C0) (b c)) (b c))

@sum$C0 = ({a c} ((b d) f))
  & @sum ~ (a (b $(:[+] $(e f))))
  &!@sum ~ (c (d e))

